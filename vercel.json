{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "APP_SCRIPT_URL": "https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}