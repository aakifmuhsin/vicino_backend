    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "APP_SCRIPT_URL": "https://script.google.com/macros/s/AKfycbz-erOFFAjAe--skS2qQZFbgyI0CBM3jJTA64SCSMKZnazG0dx9wj6R0f_itWi83gey/exec"
  }
}
