{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "APP_SCRIPT_URL": "https://script.google.com/macros/s/AKfycbwikUs0b1c0fXLUbZG4P8wOEHl_1Zqm3bstp8CKf0S01wVRyZYLEj-bY3djeEibxNBl/exec"
  }
}
